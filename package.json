{
  "name": "xtools_js",
  "version": "2.2.1",
  "description": "js tools",
  "author": "xueboren <boren.xue@outlook.com>",
  "homepage": "https://github.com/borenXue/front-packages#readme",
  "license": "MIT",
  "main": "cjs/index.js",
  "browser": "umd/index.min.js",
  "module": "es/index.js",
  "directories": {
    "lib": "./cjs",
    "doc": "./_docs"
  },
  "files": [
    "cjs",
    "_docs",
    "es",
    "umd"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/borenXue/front-packages.git"
  },
  "bugs": {
    "url": "https://github.com/borenXue/front-packages/issues"
  },
  "scripts": {
    "deploy:docs": "typedoc && cat typedoc-custom.css >> lib/_docs/assets/css/main.css && gh-pages -d _docs",
    "start": "npm run check -- --watch",
    "clean": "rm -rf es cjs umd test-reports _docs",
    "build": "npm run clean && rollup -c && npm run types:cjs && npm run types:es",
    "types:cjs": "tsc --declaration --emitDeclarationOnly --declarationMap --declarationDir cjs",
    "types:es": "tsc --declaration --emitDeclarationOnly --declarationMap --declarationDir es",
    "types:umd": "TODO: tsc --declaration --emitDeclarationOnly --declarationMap --declarationDir umd --isolatedModules false",
    "check": "tsc --noEmit",
    "prepublish": "npm run test && npm run build",
    "postpublish": "npm run deploy:docs",
    "test": "jest"
  },
  "dependencies": {
    "axios": "0.21.1",
    "core-js": "^3.8.2",
    "file-saver": "^2.0.2",
    "nested-property": "^2.0.0",
    "qs": "^6.10.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@types/file-saver": "^2.0.1",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "@types/qs": "^6.9.6",
    "gh-pages": "^3.1.0",
    "jest": "^28.1.0",
    "jest-html-reporters": "^3.0.8",
    "jest-junit": "^13.2.0",
    "jest-silent-reporter": "^0.5.0",
    "rollup": "^2.36.1",
    "rollup-plugin-terser": "^7.0.2",
    "typedoc": "0.19.2",
    "typedoc-plugin-external-module-name": "^4.0.6",
    "typedoc-plugin-localization": "^2.2.1",
    "typescript": "^4.0.5"
  }
}
